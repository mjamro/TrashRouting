{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "http://localhost:5000",
    "Audience": "http://localhost:5000",
    //"Issuer": "http://localhost:55741/",
    //"Audience": "http://localhost:55741/",
    "Key": "qwertyuiopasdfghjklzxcvbnm123456",
    "ExpireTime": {
      "Minutes": 0,
      "Hours": 24
    }
  },
  "Consul": {
    "Address": "http://localhost:8500",
    "ServiceID": "service-sync",
    "ServiceName": "service-sync",
    "ServiceAddress": "http://localhost:5030",
    "PingEndpoint": "health",
    "Tags": [ "Sync", "Data" ]
  },
  "RabbitMq": {
    "namespace": "sync",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [ "localhost" ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "Kafka": {
    "producer": {
      "bootstrapservers": "localhost:9092"
    },
    "consumer": {
      "bootstrapservers": "localhost:9092",
      "groupid": "csharp-consumer",
      "enableautocommit": true,
      "statisticsintervalms": 5000,
      "sessiontimeoutms": 6000,
      "autooffsetreset": 0,
      "enablepartitioneof": true,
      "SaslMechanism": 0,
      "SaslKerberosKeytab": "filename.keytab",
      "SaslKerberosPrincipal": "youralias@DOMAIN.COM",
      "SaslKerberosServiceName": "kafka",
      "SaslKerberosKinitCmd": "kinit -k -t %{sasl.kerberos.keytab} %{sasl.kerberos.principal}"
    }
  }
}
